{
    "main": "./lib/interface.js",   // 接口文件
    "name": "包的名称",
    "description": "包的简要说明",
    "version": "1", //版本字符串
    "keywords": [   //关键字数组
        "package",
        "example"
    ],              
    "maintainers": [   //维护者数组，每个元素要包含name，email（可选），web（可选）字段
        {
            "name": "Gordon",
            "email": "gordon.pang@hotmail.com"
        }
    ],
    "contributors": [   //贡献者数组,和维护者数组一样
        {
            "name": "Gordon",
            "email": "gordon.pang@hotmail.com",
            "web": "https://www.baidu.com/"
        }
    ],
    "bugs": {           // bug提交的邮箱或网址
       "mail": "dev@example.com",
       "web": "http://www.example.com/bugs"
    },
    "licenses": [       //许可证数组，每个元素包含type（许可证的名称）和url（连接到许可证文本的地址）字段
       {
          "type": "GPLv2",
          "url": "http://www.example.org/licenses/gpl.html"
        } 
    ],
    "repositories": [   //仓库托管地址数组，包含type（仓库类型。如git）、url（仓库地址）、path（相对于仓库等路径，可选）
        {
            "type": "git",
            "url": "http://github.com/BYVoid/mypackage.git"
        }
    ],
    "dependencies": {   //包的依赖，一个关联数组，由包名称和版本号组成
        "webkit": "1.2",
        "ssl": {
            "gnutls": ["1.0", "2.0"],
            "openssl": "0.9.8"
        }
    }
}